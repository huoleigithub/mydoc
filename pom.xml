        <version>2.2.1.RELEASE</version>
        <relativePath/>
    </parent>

    <groupId>com.vj</groupId>
    <artifactId>mydoc</artifactId>
    <packaging>pom</packaging>
    <version>1.0</version>
    <modules>
        <module>document</module>
