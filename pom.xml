<!--                <version>2.2.1.RELEASE</version>-->
<!--                <type>pom</type>-->
<!--                <scope>import</scope>-->
<!--            </dependency>-->
            <dependency>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-starter-web</artifactId>
                <version>2.2.1.RELEASE</version>
<!--                <exclusions>-->
<!--                    &lt;!&ndash; 排除自带的logback依赖 &ndash;&gt;-->
<!--                    <exclusion>-->
